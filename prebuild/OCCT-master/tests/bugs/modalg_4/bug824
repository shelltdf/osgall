if { [ catch { set info_result [OCC824 a1 a2 result] } ] } {
    puts "Faulty OCC824"
} else {
    if { [regexp {FAILED} $info_result] } {
	puts "Faulty OCC824"
    }

    set ExplodeList [explode result]
    if {[llength ${ExplodeList}] < 1} {
	puts "Faulty OCC824"
    }
}

