bopcut result

checkprops result -s 28.6438 
checkshape result
checknbshapes result -vertex 12 -edge 12 -wire 2 -face 1 -shell 0 -solid 0 -compsolid 0 -compound 1 -shape 28
checkview -display result -2d -path ${imagedir}/${test_image}.png

