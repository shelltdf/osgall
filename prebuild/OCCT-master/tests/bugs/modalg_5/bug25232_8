puts "============"
puts "OCC25232"
puts "============"
puts ""
#########################################################################
# Functionality to create solids from set of shapes
#########################################################################

cylinder cyl 10
trimv cyl cyl -20 20
mkface fcyl cyl
cone con 45 0
trimv con1 con 0 15
trimv con2 con -15 0
mkface fcon1 con1
mkface fcon2 con2
plane pl 0 0 15 0 0 1
mkface fp pl -15 15 -15 15
sphere sph 0 0 0 12
mkface fsph sph

trimv con3 con 0 30
mkface fcon3 con3
mkvolume result fcon3 fp
# 1 solid created

checkprops result -s 1706.51 
checkshape result

checknbshapes result -vertex 5 -edge 6 -wire 2 -face 2 -shell 1 -solid 1 -compsolid 0 -compound 0 -shape 17
checkview -display result -2d -path ${imagedir}/${test_image}.png
