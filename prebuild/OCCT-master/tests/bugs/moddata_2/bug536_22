
puts "========"
puts "OCC536"
puts "SAM1504"
puts "========"

vinit

restore [locate_data_file OCC536.brep] S 
checkshape S

explode S So
checkshape S_3
checkshape S_1

if [catch {bfuse result S_3 S_1}] {
    puts "Faulty OCC536"
} else {
    puts "OCC536 : OK"
    checkshape result
}

checkprops result -s 2.56034e+06
checkview -display result -2d -path ${imagedir}/${test_image}.png
