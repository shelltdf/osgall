itk_wrap_class("itk::StatisticsLabelMapFilter" POINTER)
  itk_wrap_filter_dims(restricted_dims 2+)
  foreach(d ${restricted_dims})
    foreach(t2 ${WRAP_ITK_SCALAR})
      itk_wrap_template("${ITKM_LM${d}}${ITKM_I${t2}${d}}" "${ITKT_LM${d}}, ${ITKT_I${t2}${d}}")
    endforeach()
  endforeach()
itk_end_wrap_class()
