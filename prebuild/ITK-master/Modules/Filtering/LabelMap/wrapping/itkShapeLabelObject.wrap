itk_wrap_class("itk::FixedArray")
  foreach(d ${ITK_WRAP_IMAGE_DIMS})
    math(EXPR IntegerPow "1 << ${d}")
    itk_wrap_template("P${ITKM_${ITKM_D}}${d}${IntegerPow}" "itk::Point<double, ${d}>, ${IntegerPow}")
  endforeach()
itk_end_wrap_class()

itk_wrap_class("itk::ShapeLabelObject" POINTER)
  foreach(d ${ITK_WRAP_IMAGE_DIMS})
    itk_wrap_template("${ITKM_UL}${d}" "${ITKT_UL},${d}")
  endforeach()
itk_end_wrap_class()
