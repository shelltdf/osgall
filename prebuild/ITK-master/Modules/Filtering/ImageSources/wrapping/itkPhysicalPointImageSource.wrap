itk_wrap_class("itk::PhysicalPointImageSource" POINTER)
  itk_wrap_image_filter("${WRAP_ITK_VECTOR}" 1)

  # VectorImages
  UNIQUE(to_types "UC;${WRAP_ITK_SCALAR}")
  foreach(d ${ITK_WRAP_IMAGE_DIMS})
    foreach(t ${to_types})
      itk_wrap_template("${ITKM_VI${t}${d}}" "${ITKT_VI${t}${d}}")
    endforeach()
  endforeach()

itk_end_wrap_class()
