MAKERULES_NAME = $(addsuffix $(TARGET_PLATFORM),MakeRules_)
include $(ROOT_DIR)/build/$(MAKERULES_NAME)

display:
	@echo 'TARGET_PLATFORM      = $(TARGET_PLATFORM)'
	@echo 'COLLADA_DOM_VERSION  = $(COLLADA_DOM_VERSION)'
	@echo 'BUILD                = $(BUILD)'
	@echo 'MSVC                 = $(MSVC)'
	@echo ''