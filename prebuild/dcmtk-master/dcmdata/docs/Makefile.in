#
#	Makefile for dcmdata/docs
#

@SET_MAKE@

SHELL = /bin/sh
srcdir = @srcdir@
top_srcdir = @top_srcdir@
configdir = @top_srcdir@/@configdir@

include $(configdir)/@common_makefile@

files = datadict.txt


all:

install:
	$(configdir)/mkinstalldirs $(DESTDIR)$(docdir)
	for file in $(files); do \
		$(INSTALL_DATA) $$file $(DESTDIR)$(docdir) ;\
	done

clean:
	rm -f $(TRASH)

distclean:
	rm -f $(DISTTRASH)
